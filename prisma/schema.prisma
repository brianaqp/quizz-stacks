generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int     @id @default(autoincrement())
  name  String? @db.VarChar(255)
  email String  @unique @db.VarChar(255)
  birthday DateTime @db.Date()
}

model Question {
  id Int @id @default(autoincrement())
  question String @db.VarChar(255)
}